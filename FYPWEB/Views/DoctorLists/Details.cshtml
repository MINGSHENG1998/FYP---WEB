@model FYPWEB.Models.DoctorList

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div>
    <h4>DoctorList</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Doctor's Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DocName)
        </dd>
        <dt>
            Doctor's Email
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            Doctor's Address
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt>
            State
        </dt>
        <dd>
            @Html.DisplayFor(model => model.State)
        </dd>
        <dt>
            Country
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Country)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID"><button type="button" class="btn btn-success">EDIT</button></a>
    <a asp-action="Index"><button type="button" class="btn btn-default">BACK</button></a>
</div>
